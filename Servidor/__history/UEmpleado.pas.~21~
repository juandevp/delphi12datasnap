unit UEmpleado;

interface

type
  TEmpleado = class
    fun_cidentificacion: string;
    fun_cnombres: string;
    fun_capellidos: string;
    fun_cargo: string;
    fun_dep_nid: Integer;
    fun_fecha_ingreso: string;
  end;

type
  TDependencia = class
  public
    dep_nid: Integer;
    dep_nombredependencia: string;
    constructor Create;
  end;

implementation

constructor TDependencia.Create();
begin

end;

end.
